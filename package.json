{
  "name": "mongo-indexes",
  "version": "2.0.0",
  "description": "Update a live MongoDB database's indexes based on files in source control. This was inspired from https://github.com/brandonramirez/mongo-index-sync",
  "main": "src/sync.js",
  "bin": {
    "mongo-indexes": "src/index.js"
  },
  "scripts": {
    "test": "vows",
    "package": "pkg src/index.js --output 'bin/mongo-indexes' --targets win,linux,macos"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/youngcm2/mongo-indexes.git"
  },
  "keywords": [
    "mongodb",
    "index",
    "dba",
    "database",
    "administration",
    "schema",
    "deploy"
  ],
  "author": "Chris Young",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/youngcm2/mongo-indexes/issues"
  },
  "homepage": "https://github.com/youngcm2/mongo-indexes",
  "dependencies": {
    "async": "3.0.1-0",
    "commander": "2.19.0",
    "extend": "3.0.2",
    "mongodb": "3.1.9",
    "mongoist": "https://github.com/saintedlama/mongoist.git",
    "mz": "2.7.0",
    "underscore": "1.9.1"
  },
  "devDependencies": {
    "vows": "0.8.2"
  }
}
